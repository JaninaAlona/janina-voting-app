{"ast":null,"code":"import WhoIs from './components/WhoIs.vue';\nimport LangSelect from './components/LangSelect.vue';\nimport SubmitVote from './components/SubmitVote.vue';\nexport default {\n  name: 'App',\n  components: {\n    WhoIs,\n    LangSelect,\n    SubmitVote\n  },\n\n  data() {\n    return {\n      title: \"What is your favourite programming language?\"\n    };\n  },\n\n  methods: {\n    sendVote() {\n      console.log(\"submit done\");\n      let username = document.querySelector('#userinput');\n      let progrLang = document.querySelector('.langSel[checked=\"true\"]'); // Creating a XHR object\n\n      let xhr = new XMLHttpRequest();\n      let url = \"http://127.0.0.1:8000/\"; // open a connection\n\n      xhr.open(\"POST\", url, true); // Set the request header i.e. which type of content you are sending\n\n      xhr.setRequestHeader(\"Content-Type\", \"application/json\"); // Create a state change callback\n\n      xhr.onreadystatechange = function () {\n        if (xhr.readyState === 4 && xhr.status === 200) {//Deliver results page from backend response\n        }\n      }; // Converting JSON data to string\n\n\n      var data = JSON.stringify({\n        \"username\": username.value,\n        \"progrLang\": progrLang.value\n      }); // Sending data with the request\n\n      xhr.send(data);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAQA,OAAOA,KAAP,MAAkB,wBAAlB;AACA,OAAOC,UAAP,MAAuB,6BAAvB;AACA,OAAOC,UAAP,MAAuB,6BAAvB;AAEA,eAAe;AACbC,MAAI,EAAE,KADO;AAEbC,YAAU,EAAE;AAAEJ,SAAF;AAASC,cAAT;AAAqBC;AAArB,GAFC;;AAGbG,MAAI,GAAG;AACL,WAAO;AACLC,WAAK,EAAE;AADF,KAAP;AAGD,GAPY;;AAQbC,SAAO,EAAE;AACPC,YAAQ,GAAG;AACTC,aAAO,CAACC,GAAR,CAAY,aAAZ;AACA,UAAIC,QAAO,GAAIC,QAAQ,CAACC,aAAT,CAAuB,YAAvB,CAAf;AACA,UAAIC,SAAQ,GAAIF,QAAQ,CAACC,aAAT,CAAuB,0BAAvB,CAAhB,CAHS,CAMT;;AACA,UAAIE,GAAE,GAAI,IAAIC,cAAJ,EAAV;AACA,UAAIC,GAAE,GAAI,wBAAV,CARS,CAUT;;AACAF,SAAG,CAACG,IAAJ,CAAS,MAAT,EAAiBD,GAAjB,EAAsB,IAAtB,EAXS,CAaT;;AACAF,SAAG,CAACI,gBAAJ,CAAqB,cAArB,EAAqC,kBAArC,EAdS,C